<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yww.nexus.moudles.sys.mapper.MenuMapper">

    <select id="getMenusByRoleId" resultType="com.yww.nexus.moudles.sys.entity.Menu">
        SELECT * FROM menu WHERE id IN (SELECT menu_id FROM role_menu WHERE role_id = #{roleId})
    </select>

</mapper>
